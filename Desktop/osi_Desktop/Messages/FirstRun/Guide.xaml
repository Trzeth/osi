<Grid x:Class="osi_Desktop.Messages.FirstRun.Guide"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:osi_Desktop.Messages.FirstRun"
      Background="White" Style="{DynamicResource Message}">
    <Grid.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/osi_Desktop;component/Messages/MessageStyle.xaml" ></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Grid.Resources>
    <CheckBox x:Name="httpHash_checkBox" Content="获取 HttpHash" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
    <CheckBox x:Name="httpsHash_checkBox" Content="获取 HttpsHash" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top"/>

</Grid>
